generator client {
  provider = "prisma-client-js"
}

generator erd {
  provider = "prisma-erd-generator"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model pool {
  id      String @id @default (cuid())
  title   String
  code    String @unique
  created DateTime @default(now())
}
